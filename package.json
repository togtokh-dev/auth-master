{
  "name": "auth-master",
  "version": "2.0.1",
  "description": "JWT + Basic middleware for Express & Socket.IO (ESM-first with CJS fallback)",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && node -e \"const fs=require('fs');fs.copyFileSync('dist-cjs/index.js','dist/index.cjs');\"",
    "prepare": "npm run build",
    "test": "node -e \"console.log('no tests')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/togtokh-dev/auth-master.git"
  },
  "keywords": [
    "TOGTOKH",
    "JWT",
    "middleware",
    "express",
    "socket.io",
    "typescript",
    "auth"
  ],
  "author": "TOGTOKH.DEV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/togtokh-dev/auth-master/issues"
  },
  "homepage": "https://github.com/togtokh-dev/auth-master#readme",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "response-master": "^0.0.5",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}
